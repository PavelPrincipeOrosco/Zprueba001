<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.zapp006.controller.Dialog5" xmlns:m="sap.m">
	<m:Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Aprobar/Rechazar solicitud" type="Standard"
		verticalScrolling="true">
		<m:content>
			<m:Input value="{ObsAtencionViaje}" type="Text" showValueHelp="false" enabled="true" visible="true" width="100%" valueHelpOnly="false"
				maxLength="0"/>
		</m:content>
		<m:beginButton/>
		<m:buttons>
			<m:Button text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress"/>
			<m:Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress1"/>
		</m:buttons>
		<m:customHeader/>
		<m:endButton/>
		<m:subHeader/>
	</m:Dialog>
</mvc:View>